{
  "uid": "mlops-inference",
  "title": "MLOps Inference",
  "tags": ["mlops", "inference"],
  "timezone": "browser",
  "schemaVersion": 38,
  "version": 1,
  "refresh": "30s",
  "time": {
    "from": "now-24h",
    "to": "now"
  },
  "annotations": {
    "list": [
      {
        "builtIn": 1,
        "datasource": {
          "type": "grafana",
          "uid": "-- Grafana --"
        },
        "enable": true,
        "hide": true,
        "iconColor": "rgba(0, 211, 255, 1)",
        "name": "Annotations & Alerts",
        "type": "dashboard"
      }
    ]
  },
  "panels": [
    {
      "type": "stat",
      "title": "RPS",
      "datasource": {"type": "prometheus", "uid": "prometheus"},
      "gridPos": {"x": 0, "y": 0, "w": 6, "h": 6},
      "targets": [
        {"expr": "sum(rate(bentoml_service_request_total[5m]))", "legendFormat": "rps"}
      ]
    },
    {
      "type": "stat",
      "title": "5xx Rate (rps)",
      "datasource": {"type": "prometheus", "uid": "prometheus"},
      "gridPos": {"x": 6, "y": 0, "w": 6, "h": 6},
      "targets": [
        {
          "expr": "sum(rate(bentoml_service_request_total{http_response_code=~\"5..\"}[5m]))",
          "legendFormat": "5xx"
        }
      ]
    },
    {
      "type": "stat",
      "title": "2xx Rate (rps)",
      "datasource": {"type": "prometheus", "uid": "prometheus"},
      "gridPos": {"x": 12, "y": 0, "w": 6, "h": 6},
      "targets": [
        {
          "expr": "sum(rate(bentoml_service_request_total{http_response_code=~\"2..\"}[5m]))",
          "legendFormat": "2xx"
        }
      ]
    },
    {
      "type": "stat",
      "title": "Auth Failures (4xx)",
      "datasource": {"type": "prometheus", "uid": "prometheus"},
      "gridPos": {"x": 18, "y": 0, "w": 6, "h": 6},
      "targets": [
        {
          "expr": "sum(rate(bentoml_service_request_total{http_response_code=~\"4..\"}[5m]))",
          "legendFormat": "4xx"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Requests by Status (rps)",
      "datasource": {"type": "prometheus", "uid": "prometheus"},
      "gridPos": {"x": 0, "y": 6, "w": 12, "h": 8},
      "targets": [
        {
          "expr": "sum(rate(bentoml_service_request_total{http_response_code=~\"2..\"}[5m]))",
          "legendFormat": "2xx"
        },
        {
          "expr": "sum(rate(bentoml_service_request_total{http_response_code=~\"4..\"}[5m]))",
          "legendFormat": "4xx"
        },
        {
          "expr": "sum(rate(bentoml_service_request_total{http_response_code=~\"5..\"}[5m]))",
          "legendFormat": "5xx"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Request Rate by API",
      "datasource": {"type": "prometheus", "uid": "prometheus"},
      "gridPos": {"x": 12, "y": 6, "w": 12, "h": 8},
      "targets": [
        {
          "expr": "sum(rate(bentoml_service_request_total[5m])) by (endpoint)",
          "legendFormat": "{{endpoint}}"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Error Rate (%)",
      "datasource": {"type": "prometheus", "uid": "prometheus"},
      "gridPos": {"x": 0, "y": 14, "w": 12, "h": 8},
      "targets": [
        {
          "expr": "100 * sum(rate(bentoml_service_request_total{http_response_code=~\"5..\"}[5m])) / sum(rate(bentoml_service_request_total[5m]))",
          "legendFormat": "error_rate"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "API Process CPU (cAdvisor)",
      "datasource": {"type": "prometheus", "uid": "prometheus"},
      "gridPos": {"x": 12, "y": 14, "w": 12, "h": 8},
      "targets": [
        {
          "expr": "sum(rate(container_cpu_usage_seconds_total{container_label_com_docker_compose_service=\"api\"}[5m]))",
          "legendFormat": "cpu"
        }
      ]
    },
    {
      "type": "stat",
      "title": "Dataset Drift Detected",
      "datasource": {"type": "prometheus", "uid": "prometheus"},
      "gridPos": {"x": 0, "y": 22, "w": 6, "h": 6},
      "targets": [
        {
          "expr": "max(data_drift_dataset_detected)",
          "legendFormat": "drift_detected"
        }
      ]
    },
    {
      "type": "stat",
      "title": "Drifted Feature Share",
      "datasource": {"type": "prometheus", "uid": "prometheus"},
      "gridPos": {"x": 6, "y": 22, "w": 6, "h": 6},
      "targets": [
        {
          "expr": "max(data_drift_share)",
          "legendFormat": "share"
        }
      ]
    },
    {
      "type": "stat",
      "title": "Drifted Feature Count",
      "datasource": {"type": "prometheus", "uid": "prometheus"},
      "gridPos": {"x": 12, "y": 22, "w": 6, "h": 6},
      "targets": [
        {
          "expr": "max(data_drift_feature_count)",
          "legendFormat": "drifted_features"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Drift Share Over Time",
      "datasource": {"type": "prometheus", "uid": "prometheus"},
      "gridPos": {"x": 18, "y": 22, "w": 6, "h": 6},
      "targets": [
        {
          "expr": "max(data_drift_share) by (model_name, reference_split, current_split)",
          "legendFormat": "{{model_name}} ref={{reference_split}} cur={{current_split}}"
        }
      ]
    }
  ]
}
